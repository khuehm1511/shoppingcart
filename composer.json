{
    "name": "khuehm1511/shoppingcart",
    "description": "Laravel Shoppingcart",
    "license": "MIT",
    "authors": [
        {
            "name": "Khuehm1511",
            "email": "khuehm1511@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/session": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/events": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0|~6.0|~7.0",
        "mockery/mockery": "~0.9.0",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "Khuehm1511\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Khuehm1511\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Khuehm1511\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "Khuehm1511\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
